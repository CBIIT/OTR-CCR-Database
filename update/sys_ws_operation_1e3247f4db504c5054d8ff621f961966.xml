        <http_method>GET</http_method>
        <name>getNotes</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {
	var notes = [];
	var pid = request.queryParams.project_id.toString();

	var notes_table = new GlideRecord('x_naci_otr_ccr_dat_project_notes');
	notes_table.addQuery('project_id', pid);
	notes_table.query();
	while (notes_table.next()) {
		var note = notes_table.getValue('notes').toString().slice(0,140).trim();
		if (note.length == 140) 
			note += '...';
		var date = notes_table.getValue('sys_updated_on');
		var note_id = notes_table.getValue('sys_id')
		notes.push({ note: note, date: date, note_id: note_id });
	}

	return notes;

})(request, response);]]></operation_script>
